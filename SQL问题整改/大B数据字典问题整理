  `update_id` bigint(20) NOT NULL,
  `update_time` datetime NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=utf8;


  `name` varchar(255) NOT NULL COMMENT '企业名称',
  `code` varchar(255) NOT NULL COMMENT '编码',
